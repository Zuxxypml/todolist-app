<%- include('header') -%>
<body>
  <div class="box" id="heading">
    <h1><%= listTitle %></h1>
  </div>
  <div class="box">
    <% for(i = 0; i< newItem.length; i++){ %>
    <div class="item">
      <input type="checkbox" />
      <p><%=newItem[i]%></p>
    </div>
    <%}%>
    <form class="item" action="/" method="post">
      <input
        type="text"
        name="newItem"
        id=""
        autofocus
        placeholder="New Item"
        autocomplete="off"
      />
      <button type="submit" name="listType" value="<%=listTitle%>">+</button>
    </form>
  </div>
</body>
<%- include('footer')-%>
